<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.TenementActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/relative_change"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff">

            <View
                android:id="@+id/view_fangzhurenzheng_one"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_below="@+id/text_forget"
                android:layout_marginTop="@dimen/dp_15"
                android:background="#CCCCCC" />

            <LinearLayout
                android:id="@+id/linear_down"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/view_fangzhurenzheng_one"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:visibility="gone"
                android:paddingTop="@dimen/dp_10"
                android:paddingBottom="@dimen/dp_10">

                <ImageView
                    android:layout_width="@dimen/dp_10"
                    android:layout_height="@dimen/dp_10"
                    android:layout_marginLeft="@dimen/dp_25"
                    android:src="@mipmap/add" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:text="北京市公祥街15号楼1-1302" />

                <ImageView
                    android:layout_width="@dimen/dp_10"
                    android:layout_height="@dimen/dp_10"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:src="@mipmap/ic_launcher" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_10"
            android:background="@drawable/shape_login"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/rel_name"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/text_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_toRightOf="@+id/icon_fangkejilu"
                    android:text="@string/text_name"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_16" />

                <EditText
                    android:id="@+id/et_name_tenement"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="center|right"
                    android:hint="请填写您的姓名"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_14" />

            </LinearLayout>

            <View
                android:id="@+id/view_my_fangkejilu"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_below="@+id/rel_name"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_9"
                android:background="#ececec" />


            <LinearLayout
                android:id="@+id/rel_sex_tenement"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:layout_below="@+id/view_my_fangkejilu"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_toRightOf="@+id/icon_fangkejilu"
                    android:text="性别"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_16" />

                <TextView
                    android:id="@+id/tv_sex_tenement"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_5"
                    android:layout_weight="1"
                    android:gravity="center_vertical|right"
                    android:hint="请选择性别"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_14" />

                <ImageView
                    android:layout_width="@dimen/dp_20"
                    android:layout_height="@dimen/dp_20"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dp_8"
                    android:src="@mipmap/next" />

            </LinearLayout>

            <View
                android:id="@+id/view_zhurenzheng_two"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_below="@+id/rel_sex"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_9"
                android:background="#ececec" />


            <LinearLayout
                android:id="@+id/rel_phone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:layout_below="@+id/view_zhurenzheng_two"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_toRightOf="@+id/icon_fangkejilu"
                    android:text="手机号码"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_16" />

                <EditText
                    android:id="@+id/tv_phone_number_tenement"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right|center"
                    android:hint="请填写您的号码"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_14" />

            </LinearLayout>

            <View
                android:id="@+id/view_zhurenzheng_three"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_below="@+id/rel_phone"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_9"
                android:background="#ececec" />


            <LinearLayout
                android:id="@+id/rel_relationship_tenement"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:layout_below="@+id/view_zhurenzheng_three"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_toRightOf="@+id/icon_fangkejilu"
                    android:text="与房主关系"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_16" />

                <TextView
                    android:id="@+id/tv_choose_relation"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_5"
                    android:layout_weight="1"
                    android:gravity="right|center"
                    android:text="请选择"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_14" />

                <ImageView
                    android:layout_width="@dimen/dp_20"
                    android:layout_height="@dimen/dp_20"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dp_8"
                    android:src="@mipmap/next" />
            </LinearLayout>


            <View
                android:id="@+id/view_zhurenzheng_four"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_below="@+id/rel_relationship"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_9"
                android:background="#ececec" />


            <LinearLayout
                android:id="@+id/rel_ID_number"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:layout_below="@+id/view_zhurenzheng_four"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_toRightOf="@+id/icon_fangkejilu"
                    android:text="身份证号"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_16" />

                <EditText
                    android:id="@+id/tv_ID_number_tenement"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right|center"
                    android:hint="请输入您的身份证号"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_14" />

                <ImageView
                    android:layout_width="@dimen/dp_20"
                    android:layout_height="@dimen/dp_20"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/dp_20"
                    android:layout_marginRight="@dimen/dp_15"
                    android:src="@mipmap/ewm" />

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_10"
            android:background="@drawable/shape_login"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="上传人脸照片"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_12" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="(必填)"
                    android:textColor="#B3B3B3"
                    android:textSize="@dimen/sp_12" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_20">

                <RelativeLayout
                    android:id="@+id/rl_face_photo_tenement"
                    android:layout_width="@dimen/dp_150"
                    android:layout_height="@dimen/dp_90"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:background="@drawable/shape_grey">

                    <ImageView
                        android:id="@+id/icon_face_tenement"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_add" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/icon_face_tenement"
                        android:layout_centerInParent="true"
                        android:text="人脸照片" />

                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="上传身份证照片"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_12" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="(选填)"
                    android:textColor="#B3B3B3"
                    android:textSize="@dimen/sp_12" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_20">

                <RelativeLayout
                    android:id="@+id/rl_head_one_tenement"
                    android:layout_width="@dimen/dp_150"
                    android:layout_height="@dimen/dp_90"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:background="@drawable/shape_grey">

                    <ImageView
                        android:id="@+id/icon_head_one_tenement"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_add" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/icon_head_one_tenement"
                        android:layout_centerInParent="true"
                        android:text="头像页" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_head_two_tenement"
                    android:layout_width="@dimen/dp_150"
                    android:layout_height="@dimen/dp_90"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:background="@drawable/shape_grey">

                    <ImageView
                        android:id="@+id/icon_china_tenement"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_add" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/icon_china_tenement"
                        android:layout_centerInParent="true"
                        android:text="国徽苑" />

                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="上传房屋证明"
                    android:textColor="#000000"
                    android:textSize="@dimen/sp_12" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="(选填)"
                    android:textColor="#B3B3B3"
                    android:textSize="@dimen/sp_12" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_20">

                <RelativeLayout
                    android:id="@+id/rl_houses_photo_tenement"
                    android:layout_width="@dimen/dp_150"
                    android:layout_height="@dimen/dp_90"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:background="@drawable/shape_grey">

                    <ImageView
                        android:id="@+id/icon_tenement_tenement"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_add" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/icon_tenement_tenement"
                        android:layout_centerInParent="true"
                        android:text="房屋证明" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_houses_photo_two_tenement"
                    android:layout_width="@dimen/dp_150"
                    android:layout_height="@dimen/dp_90"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:background="@drawable/shape_grey">

                    <ImageView
                        android:id="@+id/icon_house_prove_tenement"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/icon_add" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/icon_house_prove_tenement"
                        android:layout_centerInParent="true"
                        android:text="租房合同" />

                </RelativeLayout>
            </LinearLayout>

        </LinearLayout>


        <Button
            android:id="@+id/btn_submit_tenement"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_20"
            android:layout_marginBottom="@dimen/dp_40"
            android:background="@drawable/renzheng_shape"
            android:text="@string/btn_submit"
            android:textColor="#ffffff"
            android:textSize="@dimen/sp_12" />
    </LinearLayout>
</ScrollView>