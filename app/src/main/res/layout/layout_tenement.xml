<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    >

    <RelativeLayout
        android:id="@+id/relative_change"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:background="#ffffff"
        >

        <ImageView
            android:id="@+id/icon_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/icon_back"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_30"
            android:padding="@dimen/dp_10"
            />
        <TextView
            android:id="@+id/text_forget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_shenfenrenz"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_25"
            android:textSize="@dimen/sp_20"
            />
    </RelativeLayout>

    <View
        android:id="@+id/view_family_one"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_15"
        android:background="#CCCCCC"
        />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_below="@+id/view_fangzhurenzheng_one"
        >
    <RelativeLayout
        android:id="@+id/rel_name"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        >

        <TextView
            android:id="@+id/text_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_name"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="请填写您的姓名"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>
    <View
        android:id="@+id/view_my_fangkejilu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_name"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_sex"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:layout_below="@+id/view_my_fangkejilu"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="性别"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="请选择性别"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />
        <ImageView
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"
            android:src="@mipmap/next"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_20"
            />

    </RelativeLayout>

    <View
        android:id="@+id/view_zhurenzheng_two"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_sex"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_phone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:layout_below="@+id/view_zhurenzheng_two"
        >

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="手机"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_phone_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="请填写您的号码"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>

    <View
        android:id="@+id/view_zhurenzheng_three"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_phone"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_relationship"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:layout_below="@+id/view_zhurenzheng_three"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="与房主关系"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_choose_sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="租户"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />
    </RelativeLayout>


    <View
        android:id="@+id/view_zhurenzheng_four"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_relationship"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_ID_number"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:layout_below="@+id/view_zhurenzheng_four"
        >

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="身份证号"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_ID_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="请输入您的身份证号"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />
        <ImageView
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"
            android:src="@mipmap/ic_launcher"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_20"
            />

    </RelativeLayout>

    </RelativeLayout>

    <View
        android:id="@+id/view_family_two"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_15"
        android:background="#CCCCCC"
        />

       <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="上传身份证照片"
            android:textSize="@dimen/sp_12"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginLeft="@dimen/dp_20"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#B3B3B3"
            android:text="(选填)"
            android:textSize="@dimen/sp_12"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginLeft="@dimen/dp_20"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        >

        <RelativeLayout
            android:layout_width="@dimen/dp_150"
            android:layout_height="@dimen/dp_90"
            android:background="@drawable/shape_login"
            android:layout_marginLeft="@dimen/dp_20"
            >
            <ImageView
                android:id="@+id/icon_head"
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_10"
                android:src="@mipmap/ic_launcher"
                android:layout_centerInParent="true"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="头像页"
                android:layout_below="@+id/icon_head"
                android:layout_centerInParent="true"
                />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="@dimen/dp_150"
            android:layout_height="@dimen/dp_90"
            android:background="@drawable/shape_login"
            android:layout_marginLeft="@dimen/dp_10"

            >
            <ImageView
                android:id="@+id/icon_china"
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_10"
                android:src="@mipmap/ic_launcher"
                android:layout_centerInParent="true"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="国徽苑"
                android:layout_below="@+id/icon_china"
                android:layout_centerInParent="true"
                />

        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="上传租房合同、房屋证明"
            android:textSize="@dimen/sp_12"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginLeft="@dimen/dp_20"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#B3B3B3"
            android:text="(选填)"
            android:textSize="@dimen/sp_12"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginLeft="@dimen/dp_20"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        >

        <RelativeLayout
            android:layout_width="@dimen/dp_150"
            android:layout_height="@dimen/dp_90"
            android:background="@drawable/shape_login"
            android:layout_marginLeft="@dimen/dp_20"
            >
            <ImageView
                android:id="@+id/icon_tenement"
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_10"
                android:src="@mipmap/ic_launcher"
                android:layout_centerInParent="true"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="租房合同"
                android:layout_below="@+id/icon_tenement"
                android:layout_centerInParent="true"
                />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="@dimen/dp_150"
            android:layout_height="@dimen/dp_90"
            android:background="@drawable/shape_login"
            android:layout_marginLeft="@dimen/dp_10"

            >
            <ImageView
                android:id="@+id/icon_house_prove"
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_10"
                android:src="@mipmap/ic_launcher"
                android:layout_centerInParent="true"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="房屋证明"
                android:layout_below="@+id/icon_house_prove"
                android:layout_centerInParent="true"
                />

        </RelativeLayout>
    </LinearLayout>

    <Button
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        android:background="@drawable/renzheng_shape"
        android:text="@string/btn_submit"
        android:textSize="@dimen/sp_12"
        android:textColor="#ffffff"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_20"
        />
</LinearLayout>
</ScrollView>