<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.HostmanRenActivity"
    >

    <RelativeLayout
        android:id="@+id/relative_change"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_80"
        android:background="#ffffff"
        >

        <ImageView
            android:id="@+id/icon_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/icon_back"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_30"
            android:padding="@dimen/dp_10"
            />
        <TextView
            android:id="@+id/text_forget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_homerenzheng"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_25"
            android:textSize="@dimen/sp_20"
            />
    </RelativeLayout>
    <View
        android:id="@+id/view_fangzhurenzheng_one"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_2"
        android:layout_below="@+id/relative_change"
        android:layout_marginTop="@dimen/dp_15"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_white"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_below="@+id/view_fangzhurenzheng_one"
        >
        <RelativeLayout
            android:id="@+id/rel_xiaoqu"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_70"
            >

            <TextView
                android:id="@+id/text_xiaoqu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_xiaoqu"
                android:layout_toRightOf="@+id/icon_fangkejilu"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginLeft="@dimen/dp_20"
                android:textColor="#333333"
                android:textSize="@dimen/sp_16"
                />
            <TextView
                android:id="@+id/tv_choose_xiaoqu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请选择您的小区"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_40"
                android:textColor="#333333"
                android:textSize="@dimen/sp_14"
                />
            <ImageView
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:src="@mipmap/next"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_8"
                android:layout_marginTop="@dimen/dp_20"
                />

        </RelativeLayout>
        <View
            android:id="@+id/view_my_fangkejilu"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#ececec"
            android:layout_below="@+id/rel_xiaoqu"

            />


        <RelativeLayout
            android:id="@+id/rel_zhurenzheng_shenfen"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_70"
            android:layout_below="@+id/view_my_fangkejilu"
            >

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_louhao"
                android:layout_toRightOf="@+id/icon_fangkejilu"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginLeft="@dimen/dp_20"
                android:textColor="#333333"
                android:textSize="@dimen/sp_16"
                />
            <TextView
                android:id="@+id/text_shenfen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请选择您的楼号"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_40"
                android:textColor="#333333"
                android:textSize="@dimen/sp_14"
                />
            <ImageView
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:src="@mipmap/next"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_8"
                android:layout_marginTop="@dimen/dp_20"
                />

        </RelativeLayout>


        <View
            android:id="@+id/view_zhurenzheng_two"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#ececec"
            android:layout_below="@+id/rel_zhurenzheng_shenfen"

            />


        <RelativeLayout
            android:id="@+id/relative_louceng"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_70"
            android:layout_below="@+id/view_zhurenzheng_two"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_toRightOf="@+id/icon_fangkejilu"
                android:text="楼层"
                android:textColor="#333333"
                android:textSize="@dimen/sp_16" />
            <TextView
                android:id="@+id/text_louCengId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请选择您的楼层"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_40"
                android:textColor="#333333"
                android:textSize="@dimen/sp_14"
                />
            <ImageView
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:src="@mipmap/next"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_8"
                android:layout_marginTop="@dimen/dp_20"
                />

        </RelativeLayout>

        <View
            android:id="@+id/view_danyuanhao"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#ececec"
            android:layout_below="@+id/relative_louceng"

            />
        <RelativeLayout
            android:id="@+id/rel_zhurenzheng_louhao"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_70"
            android:layout_below="@+id/view_danyuanhao"
            >

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_danyuanhao"
                android:layout_toRightOf="@+id/icon_fangkejilu"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginLeft="@dimen/dp_20"
                android:textColor="#333333"
                android:textSize="@dimen/sp_16"
                />
            <TextView
                android:id="@+id/text_louhao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请选择您的单元号"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_40"
                android:textColor="#333333"
                android:textSize="@dimen/sp_14"
                />
            <ImageView
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:src="@mipmap/next"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_8"
                android:layout_marginTop="@dimen/dp_20"
                />

        </RelativeLayout>



        <View
            android:id="@+id/view_zhurenzheng_three"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#ececec"
            android:layout_below="@+id/rel_zhurenzheng_louhao"
            />

        <RelativeLayout
            android:id="@+id/rel_zhurenzheng_danyuanhao"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_70"
            android:layout_below="@+id/view_zhurenzheng_three"
            android:layout_alignParentStart="true"
            android:layout_marginTop="0dp">

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_toRightOf="@+id/icon_fangkejilu"
                android:text="@string/text_menpaihao"
                android:textColor="#333333"
                android:textSize="@dimen/sp_16" />

            <TextView
                android:id="@+id/text_danyuanhao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_40"
                android:hint="请选择您的门牌号"
                android:textColor="#333333"
                android:textSize="@dimen/sp_14" />

            <ImageView
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_8"
                android:src="@mipmap/next" />

        </RelativeLayout>

        <View
            android:id="@+id/view_zhurenzheng_four"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#ececec"
            android:layout_below="@+id/rel_zhurenzheng_danyuanhao"

            />


        <RelativeLayout
            android:id="@+id/rel_zhurenzheng_menpaihao"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_70"
            android:layout_below="@+id/view_zhurenzheng_four"
            >

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_shenfen"
                android:layout_toRightOf="@+id/icon_fangkejilu"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginLeft="@dimen/dp_20"
                android:textColor="#333333"
                android:textSize="@dimen/sp_16"
                />
            <TextView
                android:id="@+id/text_menpaihao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请选择您的身份"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_40"
                android:textColor="#333333"
                android:textSize="@dimen/sp_14"
                />
            <ImageView
                android:layout_width="@dimen/dp_20"
                android:layout_height="@dimen/dp_20"
                android:src="@mipmap/next"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_8"
                android:layout_marginTop="@dimen/dp_20"
                />

        </RelativeLayout>


    </RelativeLayout>
</RelativeLayout>