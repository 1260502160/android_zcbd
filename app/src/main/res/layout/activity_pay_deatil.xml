<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.PayDeatilActivity">

    <RelativeLayout
        android:id="@+id/relative_change"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60"
        android:background="#ffffff"
        >

        <ImageView
            android:id="@+id/icon_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/icon_back"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_20"
            android:padding="@dimen/dp_10"
            />
        <TextView
            android:id="@+id/text_forget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="缴费明细"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_25"
            android:textSize="@dimen/sp_20"
            />
    </RelativeLayout>
    <View
        android:id="@+id/view_fangzhurenzheng_one"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_2"
        android:layout_below="@+id/relative_change"
        android:layout_marginTop="@dimen/dp_1"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/white"
        >
        <ImageView
            android:layout_width="@dimen/dp_65"
            android:layout_height="@dimen/dp_65"
            android:src="@mipmap/ic_launcher"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/dp_20"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="北京公祥街社区物业有限公司"
            android:textSize="@dimen/sp_14"
            android:textColor="#000000"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/dp_18"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="-450.00"
            android:textSize="@dimen/sp_20"
            android:textColor="#000000"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/dp_14"
            />

    </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_below="@+id/view_fangzhurenzheng_one"
        >
    <RelativeLayout
        android:id="@+id/rel_xiaoqu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        >

        <TextView
            android:id="@+id/text_xiaoqu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_now_state"
          android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_choose_xiaoqu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="支付成功"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:textColor="#999999"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>
    <View
        android:id="@+id/view_my_fangkejilu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_xiaoqu"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_zhurenzheng_shenfen"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_below="@+id/view_my_fangkejilu"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_paytype"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/text_shenfen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_weichats"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:textColor="#999999"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>

    <View
        android:id="@+id/view_zhurenzheng_two"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_zhurenzheng_shenfen"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_zhurenzheng_louhao"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_below="@+id/view_zhurenzheng_two"
        >

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_pay_classify"
           android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/text_louhao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_stopcar"
            android:layout_alignParentRight="true"
           android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:textColor="#999999"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>



    <View
        android:id="@+id/view_zhurenzheng_three"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_zhurenzheng_louhao"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_zhurenzheng_danyuanhao"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_below="@+id/view_zhurenzheng_three"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_carnumber"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/text_danyuanhao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="京Q123123"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:textColor="#999999"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>

    <View
        android:id="@+id/view_zhurenzheng_four"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_zhurenzheng_danyuanhao"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_zhurenzheng_menpaihao"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_below="@+id/view_zhurenzheng_four"
        >

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_pay_explain"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/text_menpaihao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="季度 预存"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_15"
            android:textColor="#999999"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>







        <View
            android:id="@+id/view_zhurenzheng_five"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#ececec"
            android:layout_below="@+id/rel_zhurenzheng_menpaihao"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_9"
            />


        <RelativeLayout
            android:id="@+id/rel_pay_time"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_70"
            android:layout_below="@+id/view_zhurenzheng_five"
            >

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_pay_time"
                android:layout_toRightOf="@+id/icon_fangkejilu"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginLeft="@dimen/dp_20"
                android:textColor="#333333"
                android:textSize="@dimen/sp_16"
                />
            <TextView
                android:id="@+id/text_paytime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2019-09-11 12:20"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginRight="@dimen/dp_40"
                android:textColor="#333333"
                android:textSize="@dimen/sp_14"
                />

        </RelativeLayout>






        <View
            android:id="@+id/view_zhurenzheng_six"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#ececec"
            android:layout_below="@+id/rel_pay_time"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_9"
            />


        <RelativeLayout
            android:id="@+id/rel_indent_number"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_70"
            android:layout_below="@+id/view_zhurenzheng_six"
            >

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_indent_number"
                android:layout_toRightOf="@+id/icon_fangkejilu"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginLeft="@dimen/dp_20"
                android:textColor="#333333"
                android:textSize="@dimen/sp_16"
                />
            <TextView
                android:id="@+id/text_indent_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="201909111220"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginRight="@dimen/dp_40"
                android:textColor="#333333"
                android:textSize="@dimen/sp_14"
                />

        </RelativeLayout>


    </RelativeLayout>
</LinearLayout>