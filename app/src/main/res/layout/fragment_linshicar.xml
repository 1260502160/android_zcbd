<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <RelativeLayout
        android:id="@+id/rela_car"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:background="@drawable/shape_white"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_carnumber"
            android:textSize="@dimen/sp_16"
            android:textColor="#000000"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_20"
            />
        <TextView
            android:id="@+id/tv_car_number_inshicar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="请输入车牌号码"
            android:background="@null"
            android:textColor="@color/blacks"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_47"
           android:layout_centerVertical="true"
            android:textSize="@dimen/sp_16"
            />
        <ImageView
            android:id="@+id/icon_smallerror"
            android:layout_width="@dimen/dp_16"
            android:layout_height="@dimen/dp_16"
            android:src="@mipmap/close"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_20"
            android:layout_centerVertical="true"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:layout_below="@+id/icon_smallerror"
            android:layout_marginTop="@dimen/dp_28"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_20"
            />

    </RelativeLayout>

    <View
        android:id="@+id/view_car"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_15"
        android:layout_below="@+id/rela_car"
        />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view_car"
        android:background="@drawable/shape_white"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        >


    <RelativeLayout
        android:id="@+id/relative_phone_number"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"

        >
        <TextView
            android:id="@+id/text_dingdan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_cometime"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/blacks"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_18"
            />

        <TextView
            android:id="@+id/icon_mydingdan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2019-09-11  12:20"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_19"
            android:textColor="@color/grey"
            android:textSize="@dimen/sp_16"
            />
    </RelativeLayout>

    <View
        android:id="@+id/view_setting_two"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_below="@+id/relative_phone_number"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        />

    <RelativeLayout
        android:id="@+id/relative_safe_password"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_below="@+id/view_setting_two"
        >

        <TextView
            android:id="@+id/text_qchc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_18"
            android:text="@string/text_livetime"
            android:textColor="@color/blacks"
            android:textSize="@dimen/sp_16" />

        <TextView
            android:id="@+id/icon_next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2019-09-11  12:20"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_19"
            android:layout_marginRight="@dimen/dp_20"
            android:textColor="@color/grey"
            android:textSize="@dimen/sp_16"
            />

    </RelativeLayout>

    <View
        android:id="@+id/view_stop_two"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_below="@+id/relative_safe_password"
        />

    <RelativeLayout
        android:id="@+id/relative_stop_time"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_below="@+id/view_stop_two"

        >
        <TextView
            android:id="@+id/text_stop_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_stoptime"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/blacks"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_18"
            />

        <TextView
            android:id="@+id/text_stoptime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2019-09-11  12:20"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_19"
            android:textColor="@color/grey"
            android:textSize="@dimen/sp_16"
            />
    </RelativeLayout>

    <View
        android:id="@+id/view_lintwo"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_below="@+id/relative_stop_time"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        />

    <RelativeLayout
        android:id="@+id/relative_lin_money"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_55"
        android:layout_below="@+id/relative_stop_time"
        >

        <TextView
            android:id="@+id/text_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_18"
            android:text="@string/text_money"
            android:textColor="@color/blacks"
            android:textSize="@dimen/sp_16" />

        <TextView
            android:id="@+id/text_linmoney"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="￥4.00"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_19"
            android:layout_marginRight="@dimen/dp_20"
            android:textColor="#FF0000"
            android:textSize="@dimen/sp_16"
            />

    </RelativeLayout>


    </RelativeLayout>

    <Button
        android:id="@+id/btn_right_pay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/dp_78"
        android:text="@string/text_rightpay"
        android:background="@drawable/renzheng_shape"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_16"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_marginLeft="@dimen/dp_15"
        />

</RelativeLayout>