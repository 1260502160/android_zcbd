<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <RelativeLayout
        android:id="@+id/hostman_rz"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_100"
        android:background="#ffffff"
        >

        <ImageView
            android:id="@+id/icon_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/icon_back"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_30"
            android:padding="@dimen/dp_10"
            />
        <TextView
            android:id="@+id/text_forget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_homerenzheng"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_25"
            android:textSize="@dimen/sp_16"
            />

        <View
            android:id="@+id/view_fangzhurenzheng_one"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:layout_below="@+id/text_forget"
            android:layout_marginTop="@dimen/dp_15"
            android:background="#CCCCCC"
            />
        <LinearLayout
            android:id="@+id/linear_down"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            android:orientation="horizontal"
            android:layout_below="@+id/view_fangzhurenzheng_one"
            android:layout_marginTop="@dimen/dp_10"
            >
            <ImageView
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_10"
                android:src="@mipmap/ic_launcher"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginLeft="@dimen/dp_25"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="北京市公祥街15号楼1-1302"
                android:layout_marginLeft="@dimen/dp_15"
                />
            <ImageView
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_10"
                android:src="@mipmap/ic_launcher"
                android:layout_marginLeft="@dimen/dp_15"
                android:layout_marginTop="@dimen/dp_5"
                />
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linear_rz_one"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_180"
        android:orientation="vertical"
        android:background="@drawable/shape_white"
        android:layout_below="@+id/hostman_rz"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_10"
        >
        <RelativeLayout
            android:id="@+id/relative_rz_name"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_name"
                android:textColor="#000000"
                android:textSize="@dimen/sp_14"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_20"
                />
            <EditText
                android:id="@+id/edit_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请填写您的姓名"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@null"
                android:textSize="@dimen/sp_12"
                android:textColor="#B3B3B3"
                android:layout_marginRight="@dimen/dp_15"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="#CCCCCC"
                android:layout_below="@+id/edit_name"
                android:layout_marginTop="@dimen/dp_20"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relative_rz_phonenumber"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_phone_num"
                android:textColor="#000000"
                android:textSize="@dimen/sp_14"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_20"
                />
            <EditText
                android:id="@+id/edit_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请填写您的号码"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@null"
                android:textSize="@dimen/sp_12"
                android:textColor="#B3B3B3"
                android:layout_marginRight="@dimen/dp_15"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="#CCCCCC"
                android:layout_below="@+id/edit_phone"
                android:layout_marginTop="@dimen/dp_20"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relative_rz_Idcarnumber"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_60">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="身份证号"
                android:textColor="#000000"
                android:textSize="@dimen/sp_14"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_20"
                />
            <EditText
                android:id="@+id/edit_idcarnump"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="请填写您的身份证号"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@null"
                android:textSize="@dimen/sp_12"
                android:textColor="#B3B3B3"
                android:layout_marginRight="@dimen/dp_40"
                />
            <ImageView
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_10"
                android:src="@mipmap/ic_launcher"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_10"
                />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_300"
        android:background="@drawable/shape_white"
        android:layout_below="@+id/linear_rz_one"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_10"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:text="上传身份证照片"
            android:textSize="@dimen/sp_12"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginLeft="@dimen/dp_20"
            />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#B3B3B3"
                android:text="(选填)"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginLeft="@dimen/dp_20"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            >

            <RelativeLayout
                android:layout_width="@dimen/dp_140"
                android:layout_height="@dimen/dp_85"
                android:background="@drawable/shape_grey"
                android:layout_marginLeft="@dimen/dp_20"
                >
                <ImageView
                    android:id="@+id/icon_head"
                    android:layout_width="@dimen/dp_10"
                    android:layout_height="@dimen/dp_10"
                    android:src="@mipmap/ic_launcher"
                    android:layout_centerInParent="true"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="头像页"
                    android:layout_below="@+id/icon_head"
                    android:layout_centerInParent="true"
                    />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="@dimen/dp_140"
                android:layout_height="@dimen/dp_85"
                android:background="@drawable/shape_grey"
                android:layout_marginLeft="@dimen/dp_10"

                >
                <ImageView
                    android:id="@+id/icon_china"
                    android:layout_width="@dimen/dp_10"
                    android:layout_height="@dimen/dp_10"
                    android:src="@mipmap/ic_launcher"
                    android:layout_centerInParent="true"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="国徽苑"
                    android:layout_below="@+id/icon_china"
                    android:layout_centerInParent="true"
                    />

            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/dp_20"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:text="上传房屋证明"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginLeft="@dimen/dp_20"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#B3B3B3"
                android:text="(选填)"
                android:textSize="@dimen/sp_12"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginLeft="@dimen/dp_20"
                />

        </LinearLayout>

        <RelativeLayout
            android:layout_width="@dimen/dp_150"
            android:layout_height="@dimen/dp_90"
            android:background="@drawable/shape_grey"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_10"

            >
            <ImageView
                android:id="@+id/icon_home_zm"
                android:layout_width="@dimen/dp_10"
                android:layout_height="@dimen/dp_10"
                android:src="@mipmap/ic_launcher"
                android:layout_centerInParent="true"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="房屋证明"
                android:layout_below="@+id/icon_home_zm"
                android:layout_centerInParent="true"
                />

        </RelativeLayout>


    </LinearLayout>

    <Button
        android:layout_width="@dimen/dp_345"
        android:layout_height="@dimen/dp_45"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/dp_5"
        android:background="@drawable/renzheng_shape"
        android:text="@string/btn_submit"
        android:textSize="@dimen/sp_16"
        android:textColor="#ffffff"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        />

</RelativeLayout>

</ScrollView>