<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#CCCCCC"
    >

    <RelativeLayout
        android:id="@+id/relative_change"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:background="#ffffff"
        >

        <ImageView
            android:id="@+id/icon_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/icon_back"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_30"
            android:padding="@dimen/dp_10"
            />
        <TextView
            android:id="@+id/text_forget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_setting"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_25"
            android:textSize="@dimen/sp_20"
            />
    </RelativeLayout>
    <View
        android:id="@+id/view_fangzhurenzheng_one"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_below="@+id/relative_change"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_150"
        android:orientation="vertical"
        android:background="#ffffff"
        >
        <ImageView
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:src="@mipmap/ic_launcher"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/dp_20"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="北京公祥街社区物业有限公司"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/dp_20"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="-450.00"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/dp_15"
            />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"
        />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_below="@+id/view_fangzhurenzheng_one"
        >
    <RelativeLayout
        android:id="@+id/rel_name"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        >

        <TextView
            android:id="@+id/text_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="当前状态:"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="支付成功"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>
    <View
        android:id="@+id/view_my_fangkejilu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_name"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_sex"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:layout_below="@+id/view_my_fangkejilu"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="支付方式:"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="微信"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>

    <View
        android:id="@+id/view_zhurenzheng_two"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_sex"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_phone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:layout_below="@+id/view_zhurenzheng_two"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="缴费分类:"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_phone_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="停车费"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>

    <View
        android:id="@+id/view_zhurenzheng_three"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_phone"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />

    <RelativeLayout
        android:id="@+id/rel_ID"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:layout_below="@+id/view_zhurenzheng_three"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="车牌号码:"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />

        <TextView
            android:id="@+id/tv_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:text="京Q123123"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rel_relationship"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:layout_below="@+id/rel_ID"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="缴费说明:"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_choose_sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="季度 预存"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>

    <View
        android:id="@+id/view_zhurenzheng_four"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#ececec"
        android:layout_below="@+id/rel_relationship"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_9"
        />


    <RelativeLayout
        android:id="@+id/rel_ID_number"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_70"
        android:layout_below="@+id/view_zhurenzheng_four"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="支付时间:"
            android:layout_toRightOf="@+id/icon_fangkejilu"
            android:layout_marginTop="@dimen/dp_30"
            android:layout_marginLeft="@dimen/dp_20"
            android:textColor="#333333"
            android:textSize="@dimen/sp_16"
            />
        <TextView
            android:id="@+id/tv_ID_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2019-09-11 12:20"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_40"
            android:textColor="#333333"
            android:textSize="@dimen/sp_14"
            />

    </RelativeLayout>




        <View
            android:id="@+id/view_zhurenzheng_five"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#ececec"
            android:layout_below="@+id/rel_ID_number"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_marginRight="@dimen/dp_9"
            />


        <RelativeLayout
            android:id="@+id/rel_Dingdan_number"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_70"
            android:layout_below="@+id/view_zhurenzheng_five"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="订单编号:"
                android:layout_toRightOf="@+id/icon_fangkejilu"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginLeft="@dimen/dp_20"
                android:textColor="#333333"
                android:textSize="@dimen/sp_16"
                />
            <TextView
                android:id="@+id/tv_Dingdan_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="201909110212"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_40"
                android:textColor="#333333"
                android:textSize="@dimen/sp_14"
                />

        </RelativeLayout>
    </RelativeLayout>
</LinearLayout>
